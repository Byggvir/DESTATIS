#!/bin/bash

DEBUG=OFF

# Color for output

red=$(tput setaf 1)
green=$(tput setaf 2)
blue=$(tput setaf 4)
reset=$(tput sgr0)

# Download with a session link.

SED="$(dirname $0)/sed"
AWK="$(dirname $0)/awk"
DAT="$(dirname $0)/../data"
#TMP=$(mktemp -d)
TMP=/tmp
TAB=23211-0004

sed -f "${SED}/${TAB}.sed" "${DAT}/${TAB}.csv" \
| awk -F ',' -f "${AWK}/${TAB}.awk" \
> "/tmp/${TAB}.csv"


#mksql
